%thead
  %tr
    %td
    - for p in @properties
      %td.property
        %a{href: "/properties/#{p.property.id}"}= p.property.name
%tbody
  - for s in @spaces
    %tr
      %td.space
        %a{href: "/spaces/#{s.space.id}"}= s.space.name
      - for p in @properties
        - value = @traits[s.space.id][p.property.id]
        %td.value
          - if value
            %a{href: "/traits/#{value[0]}"}= value[1]